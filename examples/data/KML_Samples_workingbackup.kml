<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
    <Document>
        <name>KML Samples</name>
        <visibility>1</visibility>
        <open>1</open>
        <description>Unleash your creativity with the help of these examples!</description>
<Folder>
                <Placemark>
                    <name>Click to turn globe counter shild side to Europe</name>
                    <visibility>1</visibility>
                    <LookAt>
                        <longitude>43</longitude>
                        <latitude>43</latitude>
                        <altitude>2000000</altitude>
                        <heading>0</heading>
                        <tilt>45</tilt>
                        <range>50000</range>
                    </LookAt>
                    <styleUrl>#transYellowPoly</styleUrl>
                    <MultiGeometry>
                        <Polygon>
                            <extrude>1</extrude>
                            <altitudeMode>relativeToGround</altitudeMode>
                            <outerBoundaryIs>
                                <LinearRing>
                                    <coordinates>
                                    17,52,1000
                                    18,52,1000
                                    18,51,1000
                                    17,51,1000
                                    17,52,1000
                                    </coordinates>
                               </LinearRing>
                            </outerBoundaryIs>
                        </Polygon>
                        <Polygon>
                            <extrude>1</extrude>
                            <altitudeMode>relativeToGround</altitudeMode>
                            <outerBoundaryIs>
                                <LinearRing>
                                    <coordinates>
                                    17,50,1000
                                    18,50,1000
                                    18,49,1000
                                    17,49,1000
                                    17,50,1000
                                    </coordinates>
                                </LinearRing>
                            </outerBoundaryIs>
                        </Polygon>
                    </MultiGeometry>
                </Placemark>
            </Folder>
        <Folder>
            <name>Sentinel-2 Imagery</name>
            <visibility>1</visibility>
            <GroundOverlay>
                <name>Sentinel-2 Image 6</name>
                <Icon>
		<href>data/S2A_OPER_MTD_L1C_TL_EPA__20150813T101657_A000734_T32IBM.tif.png</href>
		<refreshMode>onExpire</refreshMode>
		<viewBoundScale>0.75</viewBoundScale>
	</Icon>
	<LatLonBox>
		<north>45.4704200301912</north>
		<south>40.10938303681844</south>
		<east>50.72932466557656</east>
		<west>36.68532041724676</west>
	</LatLonBox>
                <TimeStamp>
                    <when>2015-08-13T10:16:57</when>
                </TimeStamp>
                <Region>
                    <LatLonAltBox>
		<north>45.4704200301912</north>
		<south>40.10938303681844</south>
		<east>50.72932466557656</east>
		<west>36.68532041724676</west>
                        <minAltitude>0</minAltitude>
                        <maxAltitude>1000000</maxAltitude>
                        <altitudeMode>clampToGround</altitudeMode>
                    </LatLonAltBox>
                </Region>
            </GroundOverlay>
        </Folder>
        <Folder>
            <name>Screen Overlays</name>
            <description>Examples of screen overlays</description>
            <ScreenOverlay>
                <name>Absolute Positioning: Top left</name>
                <Icon>
                    <href>data/esa_logo_min.png</href>
                </Icon>
                <overlayXY x="0" y="1" xunits="fraction" yunits="fraction"/>
                <screenXY x="0" y="1" xunits="fraction" yunits="fraction"/>
                <rotationXY x="0" y="0" xunits="fraction" yunits="fraction"/>
                <size x="0" y="0" xunits="fraction" yunits="fraction"/>
            </ScreenOverlay>
        </Folder>

   </Document>
</kml>
